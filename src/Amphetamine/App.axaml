<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="Amphetamine.App">
	<Application.Styles>
		<FluentTheme Mode="Light" />
	</Application.Styles>
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="avares://Amphetamine/Converters/Converters.axaml" />
				<ResourceInclude Source="avares://Amphetamine/Resources/Styles/AllStyles.axaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>